<!DOCTYPE html>
<html>
<head>
<meta http-equiv="content-type" content="text/html; charset={+IO.encoding+}" />
{! Link.base = ""; /* all generated links will be relative to this */ !}
<title>HiloJS API Reference</title>
<meta name="generator" content="JsDoc Toolkit"/>
<link href="../css/bootstrap.min.css" rel="stylesheet">
<style type="text/css">
{+include("static/default.css")+}
</style>
<link href="../css/bootstrap-responsive.min.css" rel="stylesheet">
</head>
  
<body>
{+include("static/header.html").replace(/%link.base%/g, Link.base)+}
  
<div class="container-fluid">
  <div class="row-fluid">
    <div class="span3">
      {+publish.classesIndex+}
    </div>
    <div class="span9">
      <h1>全部类</h1><br/>

      <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th><h4>类</h4></th>
          <th><h4>说明</h4></th>
        </tr>
      </thead>
      <tbody>
        
      <for each="thisClass" in="data">
      {!
        if (thisClass.alias != "_global_")
        {
          output += "<tr><td style='padding:8px;'><h4>";
          output += new Link().toSymbol(thisClass.alias);
          output += "</h4></td><td style='padding:8px;'>";
          output += resolveLinks(summarize(thisClass.classDesc));
          output += "</td></tr>";
        }
      !}
      </for>

      </tbody>
    </table>

    </div>
  </div>

  <hr>
  <footer>
    <p style="text-align:center;">&copy; HiloJS 2014</p>
  </footer>

</div>

<script src="../js/jquery.min.js"></script>
<script src="../js/bootstrap.min.js"></script>
</body>
</html>